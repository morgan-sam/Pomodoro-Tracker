{
	"name": "pomodoro-tracker",
	"version": "1.0.0",
	"description": "",
	"main": "server.js",
	"scripts": {
		"start": "node server.js",
		"dev": "npm run server & chromium http://localhost:8000",
		"server": "nodemon server.js",
		"init-db": "cat ./config/init.sql | heroku pg:psql postgresql-sinuous-80299 --app pomodoro-tracker-app",
